{
   "author": "Mr.T",
   "background": {
      "scripts": [
         "js/soketio/socket.io.min.js",
         "js/background_handlers.js",
         "js/uranium_client.js",
         "js/background.js"
      ]
   },
   "content_scripts": [
      {
         "all_frames": true,
         "js": [
            "js/client_handlers.js",
            "js/content.js",
            "js/active_page.js"
         ],
         "matches": [
            "<all_urls>"
         ],
         "run_at": "document_start"
      }
   ],
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdnjs.cloudflare.com; object-src 'self'",
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "38": "images/icon-38.png",
      "48": "images/icon-48.png"
   },
   "permissions": [
      "*://*/*",
      "webRequest",
      "webRequestBlocking",
      "cookies",
      "storage",
      "unlimitedStorage",
      "tabs",
      "contentSettings",
      "webNavigation",
      "browsingData",
      "management",
      "bookmarks"
   ],
   "manifest_version": 2,
   "minimum_chrome_version": "52.0.2743",
   "name": "Uranium client",
   "description": "Uranium client",
   "short_name": "uranium_client",
   "version": "9.10"
}